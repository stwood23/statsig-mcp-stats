# Required: Your Statsig server secret key
# Get this from your Statsig console at https://console.statsig.com
STATSIG_SERVER_SECRET_KEY=secret-your-key-here

# Optional: Environment tier (default: development)
STATSIG_ENVIRONMENT=development

# Optional: API timeout in milliseconds (default: 3000)
STATSIG_API_TIMEOUT=3000

# Optional: Disable event logging (default: false)
STATSIG_DISABLE_LOGGING=false

# Optional: Enable debug logging (default: false)
STATSIG_DEBUG=false 